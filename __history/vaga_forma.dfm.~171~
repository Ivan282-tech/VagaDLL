      'select R.SIFRA_RADNIKA,'#13#10'       R.GRUPA,'#13#10'       R.RADNIK,'#13#10'    ' +
      '   R.PRAVA,'#13#10'       R.KORISNICKO_IME,'#13#10'       R.LOZINKA,'#13#10'      ' +
      ' R.ULOGOVAN,'#13#10'       R.ADRESA,'#13#10'       R.TELEFON,'#13#10'       R.MOBI' +
      'LNI,'#13#10'       R.MESTO,'#13#10'       R.BRLK,'#13#10'       R.IDOPSTINE,'#13#10'    ' +
