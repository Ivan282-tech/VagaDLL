      'E,'#13#10'       R.DEF_KUPAC,'#13#10'       R.CENOVNIK_IZMENA,'#13#10'       R.AP_' +
      'SIFRA_ROBE_UCESCE,'#13#10'       R.LBOFARMACEUTA,'#13#10'       R.IDSTOLA,'#13#10 +
      '       R.SKIN_MP,'#13#10'       k.username as izis_username,'#13#10'       k' +
      '.passw as izis_password,'#13#10'       P.NAZIV,'#13#10'       P.VP_MP,'#13#10'    ' +
      '   P.OTPREMNICA_VP_MP,'#13#10'       P.PRIJEMNICA_VP_MP,'#13#10'       P.ID_' +
      'DEF_MG_MP,'#13#10'       P.DEF_KUPAC_MP,'#13#10'       P.REZ_VP,'#13#10'       P.R' +
      'EZ_MP,'#13#10'       A.SIFRA_ROBE,'#13#10'       A.CENAMP ,'#13#10'       l.id_rol' +
      'e ,'#13#10'       l.id as id_role_primar'#13#10'from TBLRADNICI R'#13#10'left join' +
      ' TBLRADNICI_PROFIL P on R.ID_PROFILA = P.ID'#13#10'left join TBLARTIKL' +
      'I A on R.AP_KOD_ART_P = A.KOD'#13#10'left join izis_korisnici k on k.l' +
      'bo=r.lbofarmaceuta'#13#10'left join TROLE_RADNIK l on r.sifra_radnika ' +
      '= l.sifra_radnika and l.id_softvera = 2'#13#10'where R.SIFRA_RADNIKA =' +
      ' :SR'
