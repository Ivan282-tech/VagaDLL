  object sql_fis: TSQLDataSet
    SchemaName = 'sysdba'
    CommandText = 
      'select F.ID,'#13#10'       F.FISKAL,'#13#10'       F.SIFRA_DEFAULT_KUPCA,'#13#10' ' +
      '      F.SPECIJALNI_SLUCAJ,'#13#10'       F.SIRINA_DUGMETA,'#13#10'       F.V' +
      'ISINA_DUGMETA,'#13#10'       F.SIRINA_DUGMETA_STO,'#13#10'       F.VISINA_DU' +
      'GMETA_STO,'#13#10'       F.AUTO_DATUM,'#13#10'       F.ROBNO,'#13#10'       F.DEF_' +
      'MP_MG,'#13#10'       F.PERIOD_SLANJA_PODATAKA,'#13#10'       F.SIFRA_ROBE_VI' +
      'P,'#13#10'       F.SIFRA_ROBE_TELENOR,'#13#10'       F.SIFRA_ROBE_TELEKOM,'#13#10 +
      '       F.SIFRA_ROBE_GLOBAL,'#13#10'       F.SIFRA_ROBE_VECTON,'#13#10'      ' +
      ' F.SIFRA_ROBE_MTELA,'#13#10'       F.SMS_SERVIS_ADRESA,'#13#10'       F.SMS_' +
      'SERVIS_PORT,'#13#10'       F.VREME_KRETANJA,'#13#10'       M.SMS_SERVIS_TERM' +
      'INAL,'#13#10'       M.SMS_SERVIS_TERMINAL_PASSWORD,'#13#10'       F.SMS_SERV' +
      'IS_TERMINAL_FIRMWARE,'#13#10'       F.SMS_OPERATER_VIP,'#13#10'       F.SMS_' +
      'OPERATER_TELENOR,'#13#10'       F.SMS_OPERATER_TELEKOM,'#13#10'       F.SMS_' +
