  object sdsArtikli: TSQLDataSet
    CommandText = 
      'select A.KOD,'#13#10'       A.ARTIKAL,'#13#10'       V.ID_VAGE,'#13#10'       V.KO' +
      'D,'#13#10'       case'#13#10'         when V.PLU is not null then V.PLU'#13#10'   ' +
      '      else A.KOD_INFO'#13#10'       end as PLU,'#13#10'       (select C.CENA' +
      #13#10'        from cena_artikla_iz_cenovnika(A.SIFRA_ROBE, (select m' +
      '.id_cenovnik from tblmagacin m where m.sifra_magacina = :sm), :D' +
      'ATUM) C)'#13#10'from TBLARTIKLI A'#13#10'left join TBLARTIKLI_VAGA V on A.SI' +
      'FRA_ROBE = V.SIFRA_ROBE and'#13#10'      (V.ID_VAGE = :ID or :ID = -1)' +
      #13#10'where A.VAGA = 1'#13#10'and (select C.CENA'#13#10'        from cena_artikl' +
      'a_iz_cenovnika(A.SIFRA_ROBE, (select m.id_cenovnik from tblmagac' +
      'in m where m.sifra_magacina = :sm), :DATUM) C) > 0'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'sm'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'DATUM'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'ID'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'ID'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'sm'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'DATUM'
        ParamType = ptInput
