      'D,'#13#10'       R.ROLENAME,'#13#10'       R.ID_DEF_MG_MP,'#13#10'       R.DEF_KUP' +
      'AC_MP,'#13#10'       R.PRAVO_PLACANJA,'#13#10'       R.MAIL,'#13#10'       R.SMPTH' +
      'OST,'#13#10'       R.SMPTPORT,'#13#10'       R.LBO,'#13#10'       R.IME,'#13#10'       R' +
      '.PREZIME,'#13#10'       R.PRAVA_MG,'#13#10'       R.IDSEDISTE,'#13#10'       R.ID_' +
      'DEF_ULAZ_DOK,'#13#10'       R.ID_DEF_POVRAT_DOB,'#13#10'       R.CONTROL_PAN' +
      'EL,'#13#10'       R.BRZDRAVSTVENE_KNJIZICE,'#13#10'       R.PRAVA_OTKUP,'#13#10'  ' +
      '     R.DEF_FONT_MP,'#13#10'       R.SORTIRANJE_MP,'#13#10'       R.RFID,'#13#10'  ' +
      '     R.ID_PROFILA,'#13#10'       R.SIFRA,'#13#10'       R.IDPROFIL,'#13#10'       ' +
      'R.PRG_VP,'#13#10'       R.DEF_MG_SERVIS,'#13#10'       R.AP_ID_MG_P,'#13#10'      ' +
      ' R.AP_ID_PODA,'#13#10'       R.AP_KOD_ART_P,'#13#10'       R.ID_DEF_MG_USLUG' +
      'E,'#13#10'       R.PASSWORD_CHANGE_DATE,'#13#10'       R.ZAKLJUCANO,'#13#10'      ' +
